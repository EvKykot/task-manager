{
  "name": "Task-Manager",
  "version": "0.1.0",
  "description": "Task Manager (ToDo)",
  "scripts": {
    "babel-node": "babel-node --presets babel-preset-es2015",
    "server": "nodemon --watch ./src/server/ ./src/server/boot/app/app.js | bunyan",
    "client": "nodemon --watch ./webpack-client.config.dev.js --watch ./src/client-dev-server/ ./src/client-dev-server/client-dev-server.js",
    "build-client": "webpack --progress --config webpack-client.config.build.js",
    "build-server-compact": "webpack --progress --config webpack-server.config.build.js",
    "build-server": "babel --presets babel-preset-es2015 ./src/server --out-dir ./build/server",
    "build": "gulp clean && npm run build-client && npm run build-server && gulp copy-assets",
    "lint-client": "cd src/client && eslint --fix --ext=jsx --ext=js .",
    "lint-server": "cd src/server && eslint --fix --ext=jsx --ext=js .",
    "lint-client-nofix": "cd src/client && eslint --ext=jsx --ext=js .",
    "lint-server-nofix": "cd src/server && eslint --ext=jsx --ext=js .",
    "lint": "npm run lint-client && npm run lint-server",
    "lint-nofix": "npm run lint-client-nofix && npm run lint-server-nofix"
  },
